{
   "background": {
      "persistent": false,
      "scripts": [ "TabShepherd.js" ]
   },
   "description": "Group tabs into windows by URL pattern.",
   "manifest_version": 2,
   "minimum_chrome_version": "9.0.0.0",
   "name": "Tab Shepherd",
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "permissions": [ "storage", "tabs" ],
   "omnibox": {
     "keyword": "ts"
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0"
}
